{
  "name": "api-scaffolding",
  "version": "0.0.5",
  "description": "Generate API functions with types using schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "bin": {
    "api-scaffolding-types": "dist/generate.js"
  },
  "scripts": {
    "build": "npx tsc",
    "types": "npx ts-node generate -i ./example/api.schema.ts -o ./example/output.ts"
  },
  "author": "Sokolov Yevhenii",
  "devDependencies": {
    "@types/node": "^18.7.14",
    "prettier": "2.7.1",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sokoloveugene/api-scaffolding"
  }
}
