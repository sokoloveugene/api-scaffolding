{
  "name": "api-scaffolding",
  "version": "0.0.7",
  "description": "Generate API functions with types using schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "files": [
    "dist"
