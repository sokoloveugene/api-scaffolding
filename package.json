    "ppostbuild": "tsc src/generate.ts"
  },
  "author": "Sokolov Yevhenii",
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/node": "^18.7.14",
    "parcel": "^2.7.0",
    "prettier": "2.7.1",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sokoloveugene/api-scaffolding"
  }
}
